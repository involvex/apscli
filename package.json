{
  "$schema": "http://json.schemastore.org/package",
  "name": "@involvex-org/apscli",
  "version": "0.0.3",
  "public": "true",
  "description": "Advanced Powershell Command Line Interface -  A PS CLI",
  "keywords": [
    "terminal",
    "cli",
    "command line",
    "powershell",
    "ps",
    "windows",
    "blessed",
    "blessed-contrib"
  ],
  "homepage": "https://github.com/involvex/",
  "repository": {
    "type": "git",
    "url": "https://github.com/involvex/apscli.git"
  },
  "license": "./LICENSE",
  "author": "Involvex",
  "type": "commonjs",
  "main": "src/index.js",
  "bin": "dist/apscli.js",
  "files": [
    "dist",
    "src",
    "server.js",
    "node_modules"
  ],
  "scripts": {
    "build": "npx esbuild src/index.js --bundle --platform=node --outfile=dist/apscli.js --external:term.js --external:pty.js --external:events --external:blessed --external:blessed-contrib --format=cjs && npx pkg dist/apscli.js --target node18-win-x64 --output dist/apscli.exe",
    "bundle": "npx esbuild src/index.js --bundle --platform=node --outfile=dist/apscli.js --external:term.js --external:pty.js --external:events --external:blessed --external:blessed-contrib --format=cjs && npx pkg dist/apscli.js --target node18-win-x64 --output dist/apscli.exe",
    "dev": "pwsh -c clear && nodemon src/index.js",
    "format": "prettier --write \".\"",
    "format:check": "prettier --check \".\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node src/index.js",
    "start:web": "node server.js",
    "test": "mocha src/tests/test.js"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^1.0.11",
    "bootstrap-react": "^0.3.1",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "create-license": "^1.0.2",
    "eslint-ignore": "^1.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-license-header": "^0.8.0",
    "eslint-plugin-prettier": "^5.5.4",
    "express": "^5.1.0",
    "figlet": "^1.6.0",
    "license-header": "^0.0.1-security",
    "next": "15.5.3",
    "nodemon": "^3.1.10",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-sort-json": "^4.1.1",
    "ps": "^1.0.0",
    "ps-ui": "^1.0.12",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "socket.io": "^4.8.1",
    "systeminformation": "^5.27.8",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "esbuild": "^0.25.9",
    "eslint": "^9.35.0",
    "eslint-plugin-html": "^8.1.3",
    "mocha": "^11.7.2",
    "prettier": "^3.6.2"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "node": ">=22.0.0"
}